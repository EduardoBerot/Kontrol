{
  "expo": {
    "name": "Kontrol",
    "slug": "financa",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "financa",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/Kontrol.png",
    "splash": {
      "image": "./assets/Kontrol.png",
      "backgroundColor": "#3560e3",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "softwareKeyboardLayoutMode": "resize",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.eduardoberot.Kontrol"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7d0395df-6425-4333-a59f-5e777a63017f"
      }
    }
  }
}
